<div class='weatherWidget'>
  {{#each partials.weatherContext}}
    <div class='location'>
      <h3>{{location.name}}</h3>
      <a href='{{location.forecastUrl}}'>
        <img sec='{{iconUrl}}' alt='{{weather}}' />
        {{weather}},{{temp}}
      </a>
    </div>
  {{/each}}
  <small>Source:
    <a href='http://www.weather.gov/documentation/services-web-api'>
      National Weather Service</a></small>
</div>